# .env.example - Copy to .env and fill in values

# Database
DATABASE_URL=postgresql+asyncpg://infinity_os:secure_password@localhost/infinity_os

# Security - CHANGE THIS IN PRODUCTION (use: openssl rand -hex 32)
JWT_SECRET_KEY=your-super-secure-jwt-secret-key-change-in-production

# Application
ENVIRONMENT=development
PORT=8000
WORKERS=1
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Logging
LOG_LEVEL=INFO
SQL_ECHO=false
