# Infinity OS Backend Configuration
# Copy to .env and update values

# Database
DATABASE_URL=postgresql+asyncpg://infinity:your_password@localhost:5432/infinity_os

# Authentication
JWT_SECRET_KEY=generate-a-64-char-random-string-here
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:8080

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# Storage
FILE_STORAGE_ROOT=/data/files
GIT_STORAGE_ROOT=/data/repos
BUILD_OUTPUT_ROOT=/data/builds

# Redis (Valkey)
REDIS_URL=redis://localhost:6379/0

# GitHub Integration (for repo sync)
GITHUB_TOKEN=ghp_your_token_here
GITHUB_ORG=Trancendos

# LLM Providers (zero-cost first)
GROQ_API_KEY=
HF_TOKEN=
GOOGLE_API_KEY=
DEEPSEEK_API_KEY=
CLOUDFLARE_AI_TOKEN=